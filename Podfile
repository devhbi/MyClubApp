# Uncomment the next line to define a global platform for your project

platform :ios, '13.0'

def shared_pods
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Google'
  pod 'Firebase/Database'
  pod 'GoogleSignIn'
end

target 'FCEBM' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FCEBM

end

target 'MyClubApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  shared_pods

end

target 'SSFC' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  shared_pods

end

target 'UJS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  shared_pods

end
